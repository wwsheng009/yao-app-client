{
  "name": "yao-app-client",
  "version": "1.0.3",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist/esm --minify --format esm & bun run build:declaration",
    "build:bun": "bun build ./src/index.ts --outdir ./dist/bun --target bun",
    "build:tsc": "tsc",
    "build:declaration": "tsc --emitDeclarationOnly",
    "publish": "npm pack && npm publish",
    "test": "bun tests/yao.ts"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "author": "vincentwwsheng@gmail.com",
  "license": "MIT",
  "dependencies": {
    "mime": "^4.0.1",
    "tsc": "^2.0.4",
    "yao-app-ts-types": "^1.0.3"
  },
  "files": [
    "dist",
    "src/**/*.d.ts",
    "LICENSE",
    "package.json"
  ]
}